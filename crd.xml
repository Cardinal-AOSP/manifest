<?xml version="1.0" encoding="UTF-8"?>
 <manifest>
  <remote  name="aosp"
           fetch="https://android.googlesource.com/"
           review="https://android-review.googlesource.com/" />

  <default revision="refs/tags/android-8.1.0_r50"
           remote="aosp"
           sync-j="4" />

  <remote  name="crd"
           fetch="https://github.com/Cardinal-AOSP/"
           revision="oreo-mr1" />

  <remote  name="github"
           fetch="https://github.com/" />

  <remote  name="qcom"
           fetch="https://source.codeaurora.org/quic/la/"
           revision="refs/tags/LA.UM.6.4.r1-10300-8x98.0" />

<!-- Cardinal-AOSP starts -->

  <!-- Include aosp and qcom repos -->
  <include name="aosp.xml" />
  <include name="qcom.xml" />
  <!-- Repo's to exclude -->
  <include name="remove.xml" />

  <!-- Art -->
  <remove-project name="platform/art" />
  <project name="art" path="art" remote="crd" />

  <!-- Bionic -->
  <remove-project name="platform/bionic" />
  <project name="bionic" path="bionic" remote="crd" />

  <!-- Recovery -->
